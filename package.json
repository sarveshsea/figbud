    "@babel/plugin-transform-spread": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/runtime": "^7.27.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "autoprefixer": "^10.4.21",
