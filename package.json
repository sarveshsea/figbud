{
  "name": "figbud",
  "version": "1.0.0",
  "description": "Source control for FigBud, an AI-powered Figma widget that provides timestamped YouTube tutorial recommendations, automated demo creation, step-by-step guidance, and collaboration tools. Built with TypeScript, React, and Node.js, integrating Figma Widget API, YouTube Data API, and OpenAI API.",
  "main": "dist/code.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "npm run build:figma",
    "build:ui": "webpack --mode production --config webpack.ui.config.js",
    "build:code": "webpack --mode production --config webpack.code.config.js",
    "build:figma": "node scripts/build-figma-plugin.js",
    "copy-to-root": "cp dist/code.js . && cp dist/ui.html .",
    "build:vanilla": "node scripts/build-vanilla-simple.js",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "server": "tsx server/index.ts",
    "server:dev": "nodemon --exec ts-node server/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ai": "node tests/test-ai.js",
    "test:youtube": "node tests/test-youtube-timestamps.js",
    "test:validation": "node tests/test-ai-validation.js",
    "test:sessions": "node tests/test-chat-sessions.js",
    "test:all": "node tests/run-all-tests.js",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "validate:env": "node scripts/validate-env.js",
    "setup": "cp .env.example .env && npm run validate:env",
    "db:check": "node scripts/check-database-schema.js",
    "db:fix": "node scripts/fix-database-schema.js",
    "db:setup": "echo 'Please run the SQL from supabase-schema.sql in your Supabase SQL editor'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarveshsea/figbud.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sarveshsea/figbud/issues"
  },
  "homepage": "https://github.com/sarveshsea/figbud#readme",
  "dependencies": {
    "@figma/plugin-typings": "^1.116.0",
    "@mendable/firecrawl-js": "^1.29.1",
    "@once-ui-system/core": "^1.3.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.52.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "core-js": "^3.44.0",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "googleapis": "^153.0.0",
    "html-webpack-plugin": "^5.6.3",
    "lucide-react": "^0.525.0",
    "node-fetch": "^2.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "style-loader": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "ts-loader": "^9.5.2",
    "uuid": "^11.1.0",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-async-to-generator": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/plugin-transform-spread": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/runtime": "^7.27.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "eslint": "^9.31.0",
    "html-loader": "^5.1.0",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "react-dev-utils": "^12.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
