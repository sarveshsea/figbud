{
  "name": "figbud",
  "version": "1.0.0",
  "description": "Source control for FigBud, an AI-powered Figma widget that provides timestamped YouTube tutorial recommendations, automated demo creation, step-by-step guidance, and collaboration tools. Built with TypeScript, React, and Node.js, integrating Figma Widget API, YouTube Data API, and OpenAI API.",
  "main": "dist/code.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production && node scripts/inline-build.js",
    "build:vanilla": "node scripts/build-vanilla-simple.js",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ai": "node test-ai.js",
    "server:dev": "nodemon --watch server --ext ts --exec 'ts-node --project tsconfig.server.json' server/index.ts",
    "lint": "eslint src server --ext .ts,.tsx",
    "lint:fix": "eslint src server --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "start:server": "node dist/server/index.js",
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "build:server": "tsc && cp -r data dist/ 2>/dev/null || true",
    "migrate": "ts-node server/database/runMigrations.ts",
    "migrate:direct": "ts-node server/database/runMigrations.ts --direct"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarveshsea/figbud.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sarveshsea/figbud/issues"
  },
  "homepage": "https://github.com/sarveshsea/figbud#readme",
  "dependencies": {
    "@figma/plugin-typings": "^1.116.0",
    "@once-ui-system/core": "^1.3.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.52.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "core-js": "^3.44.0",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "googleapis": "^153.0.0",
    "helmet": "^8.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "openai": "^5.10.1",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^5.6.0",
    "style-loader": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "ts-loader": "^9.5.2",
    "uuid": "^11.1.0",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  },
